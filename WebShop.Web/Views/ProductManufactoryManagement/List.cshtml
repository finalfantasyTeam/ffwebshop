<div class="row">
  <div class="col-xs-12">
    <div class="box">
      <div class="box-header">
        <h3 class="box-title">Product manufactory list</h3>
      </div>
      <div class="box-body">
        <table id="tblProductManufactory" class="table table-bordered table-striped">
          <thead>
            <tr>
              <th>#</th>
              <th>Logo</th>
              <th>Name</th>
              <th>Description</th>
              <th>Country</th>
              <th>Active</th>
            </tr>
          </thead>
          
          <tbody>
            @{
              if (Model.ListProductManufactory != null)
              {
                foreach (var data in Model.ListProductManufactory)
                {
                  Html.Raw(data.Name);
                }
              }
            }
            <tr>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
            </tr>
          </tbody>
          <tfoot>
            <tr>
              <th>#</th>
              <th>Logo</th>
              <th>Name</th>
              <th>Description</th>
              <th>Country</th>
              <th>Active</th>
            </tr>
          </tfoot>
        </table>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript">
  $(document).ready(function () {
    $("#tblProductManufactory").DataTable();
  });
</script>