<div class="row">
  <div class="col-xs-12">
    <div class="box">
      <div class="box-header">
        <h3 class="box-title">Configurations</h3>
      </div>
      <div class="box-body">
        <table id="tblProductManufactory" class="table table-bordered table-striped">
          <thead>
            <tr>
              <th>#</th>
              <th>Logo</th>
              <th>Name</th>
              <th>Description</th>
              <th>Country</th>
              <th>Active</th>
            </tr>
          </thead>
          <tbody>
            @{
                if (Model.ListConfigOptions != null)
                {
                  foreach (var data in Model.ListConfigOptions)
                  {
                        <tr>
                          <td>@data.Id</td>
                          <td>@data.ManufatoryLogo</td>
                          <td>@data.Name</td>
                          <td>@data.Description</td>
                          <td>@data.ManufactoryCountry</td>
                          <td>@data.IsActive</td>
                        </tr>
                  }
                }
            }
            
          </tbody>
          <tfoot>
            <tr>
              <th>#</th>
              <th>Logo</th>
              <th>Name</th>
              <th>Description</th>
              <th>Country</th>
              <th>Active</th>
            </tr>
          </tfoot>
        </table>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript">
  $(document).ready(function () {
    $("#tblProductManufactory").DataTable();
  });
</script>