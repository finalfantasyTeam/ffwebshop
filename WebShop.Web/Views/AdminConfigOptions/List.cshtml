<div class="row">
  <div class="col-xs-12">
    <div class="box">
      <div class="box-header">
        <h3 class="box-title">Configurations</h3>
      </div>
      <div class="box-body">
        <table id="tblProductManufactory" class="table table-bordered table-striped">
          <thead>
            <tr>
              <th>#</th>
              <th>Config Key</th>
              <th>Config Value</th>
              <th>Notes</th>
              <th>Active</th>
            </tr>
          </thead>
          <tbody>
            @{
                if (Model.ListConfigOptions != null)
                {
                  foreach (var data in Model.ListConfigOptions)
                  {
                      <tr>
                        <td>
                          <a href='@Url.Action("Details", new { id = data.Id })'>@data.Id</a>
                        </td>
                        <td>
                          <a href="@Url.Action("Details", new { id = data.Id })">@data.OptionKey</a>
                        </td>
                        <td>@data.OptionValue</td>
                        <td>@data.Notes</td>
                        <td>@data.IsActive</td>
                      </tr>
                  }
                }
            }
          </tbody>
        </table>
      </div>
      <div class="box-footer">
        @Html.ActionLink(L("CreateButtonLabel"), "Create", null, new { @class = "btn btn-info" })
      </div>
    </div>
  </div>
</div>
<script type="text/javascript">
  $(document).ready(function () {
    $("#tblProductManufactory").DataTable();
  });
</script>