<div class="row">
  <div class="col-xs-12">
    <div class="box">
      <div class="box-header">
        <h3 class="box-title">User List</h3>
      </div>
      <div class="box-body">
        <table id="tblProductManufactory" class="table table-bordered table-striped">
          <thead>
            <tr>
              <th>Username</th>
              <th>Is Online</th>
              <th>Last Active Date</th>
              <th>Active</th>
            </tr>
          </thead>
          <tbody>
            @{
                if (Model.ListUsers != null)
                {
                  foreach (var data in Model.ListUsers)
                  {
                      <tr>
                        <td>
                          <a href='@Url.Action("Details", new { id = data.UserName })'>@data.UserName</a>
                        </td>
                        <td>@data.IsOnline</td>
                        <td>@data.LastActivityDate</td>
                        <td>@data.IsLockedOut</td>
                      </tr>
                  }
                }
            }
          </tbody>
        </table>
      </div>
      <div class="box-footer">
        @Html.ActionLink(L("CreateButtonLabel"), "Create", null, new { @class = "btn btn-info" })
      </div>
    </div>
  </div>
</div>
<script type="text/javascript">
  $(document).ready(function () {
    $("#tblProductManufactory").DataTable();
  });
</script>